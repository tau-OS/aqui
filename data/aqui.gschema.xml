<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="/com/fyralabs/Aqui/" id="com.fyralabs.Aqui">
    <key type="ad" name="last-viewed-location">
      <default>[0.0,0.0]</default>
      <summary>last viewed location</summary>
      <description>Coordinates of last viewed location.</description>
    </key>   
    <key type="u" name="zoom-level">
      <default>8</default>
      <summary>Zoom level</summary>
      <description>Zoom level of the map last used</description>
    </key>
    <key type="i" name="window-width">
      <default>1000</default>
      <summary>Window width</summary>
      <description>Width of the window last closed</description>
    </key>
    <key type="i" name="window-height">
      <default>800</default>
      <summary>Window height</summary>
      <description>Height of the window last closed</description>
    </key>
    <key type="b" name="maximized">
      <default>false</default>
      <summary>Maximized window</summary>
      <description>Whether the window last closed was maximized or not</description>
    </key>
   </schema>
</schemalist>
